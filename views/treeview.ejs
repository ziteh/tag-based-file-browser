<% if (items.length) { %>
<ul>
<% } %>

  <% items.forEach(function(item){ %>
    <li>
      <a href="/tags/<%= item.id %>">
        <%= item.name %>
      </a>

      <% if (item.child) { %>
        <%- include('treeview', {items: item.child}); %>
      <% } %>
    </li>
  <% }) %>

<% if (items.length) { %>
</ul>
<% } %>
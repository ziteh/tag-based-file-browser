<% if (items.length) { %>
  <ul>
    <% } %>

      <% items.forEach(function(item){ %>
        <li>
          <input type="checkbox" value="<%= item.id %>" name="tag"/>
          <%= item.name %>

          <% if (item.child) { %>
            <%- include('treeviewWithCheckbox', {items: item.child}); %>
              <% } %>
        </li>
        <% }) %>

          <% if (items.length) { %>
  </ul>
  <% } %>